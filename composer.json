{
  "name": "contributte/kernel",
  "description": "Convenient bootloader for Nette (@nette) applications.",
  "keywords": [
    "nette",
    "bootstrap",
    "bootloader",
    "kernel",
    "stack"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/kernel",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/bootstrap": "^3.2.0",
    "nette/di": "^3.1.2"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.5.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Kernel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
